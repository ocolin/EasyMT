{
    "name": "ocolin/easymt",
    "description": "Simple wrapper for evil freelancers RouterOS API client",
    "type": "library",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/ocolin/EasyEnv"
        },
        {
            "type": "vcs",
            "url": "https://github.com/ocolin/EasySNMP"
        }
    ],
    "require": {
        "php": ">=8.3",
        "evilfreelancer/routeros-api-php": "^1.5",
        "ocolin/easyenv": "^1.0",
        "ocolin/easysnmp": "^v1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.1",
        "phpstan/phpstan": "^1.11"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Ocolin\\EasyMT\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Colin Miller",
            "email": "ocolin@cruzio.com"
        }
    ]
}
